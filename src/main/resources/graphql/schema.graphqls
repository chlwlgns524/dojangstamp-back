type Query {
    orderList: [Order]
    stampList: [Stamp]
}

type Order {
    id: ID!
    createdAt: String
    content: String
    quantity: Int
    memo: String
    status: String
    stamp: Stamp
}

type Stamp {
    id: ID!
    modelName: String
    width: Float
    height: Float
    price: Float
}